<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cse214.theo.lunchtimeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.cse214.theo.lunchtimeapp.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cse214.theo.lunchtimeapp.DialogCut"
            android:theme="@style/NoTitleDialog" />
        <activity
            android:name="com.cse214.theo.lunchtimeapp.DialogAdd"
            android:label="@string/title_activity_add_student_"
            android:theme="@style/NoTitleDialog" />
        <activity
            android:name="com.cse214.theo.lunchtimeapp.DialogUpdateMoney"
            android:theme="@style/NoTitleDialog" />
        <activity
            android:name="com.cse214.theo.lunchtimeapp.DialogServed"
            android:theme="@style/NoTitleDialog" />
        <activity
            android:name="com.cse214.theo.lunchtimeapp.DialogRemove"
            android:theme="@style/NoTitleDialog" />
        <activity
            android:name="com.cse214.theo.lunchtimeapp.DialogTrade"
            android:theme="@style/NoTitleDialog">

        </activity>
    </application>

</manifest>